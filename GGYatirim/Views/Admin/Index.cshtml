            @foreach (var l in Model)
            {
                if (l.AdSoyad != null && l.EPosta != null && l.Tel != null && l.Sifre != null)
                {
                    <div class="col-lg-3 col-md-6">
                        <div class="card">
                            <div class="body">
                                <h3>100%</h3>
                                <p class="text-muted">Profil Gücü</p>
                                <div class="progress">
                                    <div class="progress-bar l-parpl" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%;"></div>
                                </div>
                                <small>Profilinizi Doldurdukça İnsanların Güvencesi Artar</small>
                            </div>
                        </div>
                    </div>
                }




                if (l.AdSoyad == null && l.EPosta == null && l.Tel == null && l.Sifre == null)
                {
                    <div class="col-lg-3 col-md-6">
                        <div class="card">
                            <div class="body">
                                <h3>1%</h3>
                                <p class="text-muted">Profil Gücü</p>
                                <div class="progress">
                                    <div class="progress-bar l-parpl" role="progressbar" aria-valuenow="1" aria-valuemin="0" aria-valuemax="100" style="width: 1%;"></div>
                                </div>
                                <small>Profilinizi Doldurdukça İnsanların Güvencesi Artar</small>
                            </div>
                        </div>
                    </div>
                }


                if (l.AdSoyad == null || l.EPosta == null)
                {
                    <div class="col-lg-3 col-md-6">
                        <div class="card">
                            <div class="body">
                                <h3>30%</h3>
                                <p class="text-muted">Profil Gücü</p>
