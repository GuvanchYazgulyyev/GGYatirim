                                    <div class="progress-bar l-parpl" role="progressbar" aria-valuenow="1" aria-valuemin="30" aria-valuemax="100" style="width: 30%;"></div>
                                </div>
                                <small>Profilinizi Doldurdukça İnsanların Güvencesi Artar</small>
                            </div>
                        </div>
                    </div>
                }



                if (l.Tel == null)
