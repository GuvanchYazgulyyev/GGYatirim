@model GGYatirim.Models.Sinif.DbEmlakTipi
@{
    ViewBag.Title = "AdmEmlakTipiEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h2> Emlak Tipi Ekle</h2>
<div class="col-md-12">
    @using (Html.BeginForm("AdmEmlakTipiEkle", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <form class="form-group" method="post">

            Hizmet Ad
            @Html.TextBoxFor(x => x.EmlakTipAd, new { @class = "form-control" })


            @Html.HiddenFor(x => x.EmlakTipiID)

            <button class="button border with-icon">Yeni Emlak Tipi Ekle</button>

        </form>
    }
</div>
