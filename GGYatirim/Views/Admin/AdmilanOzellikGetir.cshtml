@model GGYatirim.Models.Sinif.DbilanOzellik
@{
    ViewBag.Title = "AdmilanOzellikGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>İlan Durum Güncelle</h3>

<div style="width:50%" ; class="col-md-4 table table-hover">
    @using (Html.BeginForm("AdmilanOzellikGuncelle", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {


        <form class="form-group" method="post">

            İlan Özellik Ad
            @Html.TextBoxFor(x => x.OzellikAd, new { @class = "form-control" })



            @Html.HiddenFor(x => x.ilanOzellikID)
            <br />

            <button class="btn btn-success">Değişikliği Kaydet</button>

        </form>





    }


</div>

