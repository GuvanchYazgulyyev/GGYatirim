@model GGYatirim.Models.Sinif.DbKonum
@{
    ViewBag.Title = "AdmKonumGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Konum Güncelle</h3>

<div class="col-md-12 table table-hover">
    @using (Html.BeginForm("AdmKonumGuncelle", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {


        <form class="form-group" method="post">

            İlan Özellik Ad
            @Html.TextBoxFor(x => x.SehirAd, new { @class = "form-control" })



            @Html.HiddenFor(x => x.KonumID)
            <br />

            <button class="btn btn-success">Değişikliği Kaydet</button>

        </form>





    }


</div>

